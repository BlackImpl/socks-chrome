{
  "manifest_version": 2,

  "name": "scmplugin",
  "description": "scm chrome extention",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "permissions": [
    "tabs",
	"http://2nd.alibaba.net/scm2env/systemSync.do?method=getCurrentProject",
	"http://ops.jm.taobao.net/eagleeye/TraceSelfUnit",
	"http://2nd.alibaba.net/scm2env/systemSync.do?method=getProjectAppByHost",
	"http://*/.taobao.net/*",
	"http://*/.etao.net/*",
	"http://*/.tmall.net/*",
	"http://*/.aliyun.net/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' ; object-src 'self' "
}
